{"_id":"SYIqXKY2V05HOHNI","name":"Graceful Agility","type":"feat","img":"systems/pf2e/icons/default-icons/feats.webp","effects":[],"flags":{"core":{"sourceId":"Item.lrCZ2onxte0og5V8"},"cf":{"id":"temp_ot30365qab9","path":"Samurai Class","color":"#000000"}},"system":{"description":{"value":"<p>Your movements are gracious and agile when wielding your favorite weapons. You consider all weapons you are master with agile.</p>"},"source":{"value":""},"rules":[{"key":"AdjustStrike","mode":"add","property":"weapon-traits","value":"agile","priority":60,"definition":[{"gte":["item:proficiency:rank",3]}]}],"slug":null,"schema":{"version":0.819,"lastMigration":{"datetime":"2023-02-13T16:48:20.510+01:00","version":{"schema":0.769,"foundry":"10.291","system":"4.7.4"}}},"level":{"value":10},"traits":{"value":["hb_samurai"],"rarity":"common","custom":""},"featType":{"value":"class"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"passive"},"actions":{"value":null},"prerequisites":{"value":[]},"location":null},"_stats":{"systemId":"pf2e","systemVersion":"4.7.4","coreVersion":"10.291","createdTime":null,"modifiedTime":1676309085323,"lastModifiedBy":"jPllA8HXTTOgrI8J"},"folder":null,"sort":0,"ownership":{"default":0,"jPllA8HXTTOgrI8J":3}}
